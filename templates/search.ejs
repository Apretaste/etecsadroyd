<div class="row">
    <div class="col s12">
        <p>Buscando por "<%= search %>" en todas las provincias.</p>
    </div>

    <ul class="collection">
        <% results.forEach(function(result) { %>
            <li class="collection-item avatar">
                <i class="tiny material-icons circle">local_phone</i>
                <span class="title"><b><%= result.name %></b> - <%= result.location %></span><br>
                <p class="blue-grey-text">+53<%= result.phone %></p>
            </li>
        <% }) %>
    </ul>

    <div class="fixed-action-btn" onclick="apretaste.send({'command': 'ETECSADROYD'})">
        <a class="btn-floating btn-large green">
            <i class="large material-icons">arrow_back</i>
        </a>
    </div>
</div>
